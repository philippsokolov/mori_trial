

{% section 'content' %}

<script>
$(".show-more").click(function() {
    $('html, body').animate({
        scrollTop: $("#image_1").offset().top
    }, 2000);
});
$(document).ready(function() {
  $('.owl-carousel').owlCarousel({
        items:1,
        loop:true,
        margin:0,
        autoplay:true,
        autoplayTimeout:5000,
        autoplayHoverPause:true
  });

$('.head-content').fadeIn('slow');
})
</script>
